#include <stdio.h>

int main() {
    char grades[5];
    int score = 0;

 

    for (int i = 0; i < 5; i++)
    {
        scanf(" %c", &grades[i]);

        
        switch (grades[i]) {
        case 'A':
            score += 1;
            break;
        case 'B':
            score += 2;
            break;
        case 'C':
            score += 3;
            break;
        case 'D':
            score += 4;
            break;
       
        }
    }

    printf("%d", score);

    return 0;
}
 
